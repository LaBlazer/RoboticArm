                <Label Name="Elbow0" Background="{x:Null}" Content="Elbow0" Foreground="#FFECECEC" FontSize="16" FontWeight="Bold"/>
                <Label Name="Elbow1" Background="{x:Null}" Content="Elbow1" Foreground="#FFECECEC" FontSize="16" FontWeight="Bold" />
                <Label Name="Elbow2" Background="{x:Null}" Content="Elbow2" Foreground="#FFECECEC" FontSize="16" FontWeight="Bold"/>
                <Label Name="Base" Background="{x:Null}" Content="Base" Foreground="#FFECECEC" FontSize="16" FontWeight="Bold"/>
                <Label Name="Gripper0" Background="{x:Null}" Content="Gripper0" Foreground="#FFECECEC" FontSize="16" FontWeight="Bold"/>
                <Label Name="Gripper1" Background="{x:Null}" Content="Gripper1 " Foreground="#FFECECEC" FontSize="16" FontWeight="Bold"/>


