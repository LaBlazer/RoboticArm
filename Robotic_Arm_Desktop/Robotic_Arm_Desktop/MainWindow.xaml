            <Label Name="availableD" Content="err" HorizontalAlignment="Left" Margin="267,74,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#FF609DD1" FontWeight="Bold"/>
            <Label Name="maxUse" Content="err" HorizontalAlignment="Left" Margin="267,110,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#FF609DD1" FontWeight="Bold"/>
