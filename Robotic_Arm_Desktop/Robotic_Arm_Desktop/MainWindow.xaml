            <Slider Name="max" Minimum="0" HorizontalAlignment="Left" Margin="168,161,0,0" VerticalAlignment="Top" Width="129" ValueChanged="MaxUseSliderChange"/>
            <Label  Content="Start from :" HorizontalAlignment="Left" Margin="162,182,0,0" VerticalAlignment="Top" Foreground="#FFEAEAEA" FontSize="17" FontWeight="Bold" />
            <Label Name="startFrom"  Content="err" HorizontalAlignment="Left" Margin="267,184,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#FF609DD1" FontWeight="Bold"/>
            <Slider Name="start" HorizontalAlignment="Left" Minimum="0" Margin="168,220,0,0" VerticalAlignment="Top" Width="129" ValueChanged="StartFromSliderChange"/>
