        </Grid>
        <Grid HorizontalAlignment="Left" Height="187" Margin="670,650,0,0" VerticalAlignment="Top" Width="366" Background="#FF252526">
            <Label Content="Manual Mode:" HorizontalAlignment="Left" Margin="18,10,0,0" VerticalAlignment="Top" Foreground="#FFEAEAEA" FontSize="22" FontWeight="Bold"/>
            <Label Content="Keyboard :" HorizontalAlignment="Left" Margin="18,62,0,0" VerticalAlignment="Top" Foreground="#FFEAEAEA" FontSize="17" FontWeight="Bold"/>
