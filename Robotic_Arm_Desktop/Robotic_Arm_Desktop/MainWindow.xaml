            <Label x:Name="elb2a" Content="0" HorizontalAlignment="Left" Margin="189,137,0,0" VerticalAlignment="Top" FontSize="18" Foreground="#FF609DD1" FontWeight="Bold" />
            <Label x:Name="elb2v" Content="0" HorizontalAlignment="Left" Margin="259,137,0,0" VerticalAlignment="Top" FontSize="18" Foreground="#FF609DD1" FontWeight="Bold"/>
            <Label x:Name="elb0v" Content="0" HorizontalAlignment="Left" Margin="259,59,0,0" VerticalAlignment="Top" FontSize="18" Foreground="#FF609DD1" FontWeight="Bold"/>
            <Label x:Name="grra" Content="0" HorizontalAlignment="Left" Margin="189,176,0,0" VerticalAlignment="Top" FontSize="18" Foreground="#FF609DD1" FontWeight="Bold"/>
            <Label x:Name="grrv" Content="0" HorizontalAlignment="Left" Margin="259,176,0,0" VerticalAlignment="Top" FontSize="18" Foreground="#FF609DD1" FontWeight="Bold" RenderTransformOrigin="0.561,-0.078"/>
