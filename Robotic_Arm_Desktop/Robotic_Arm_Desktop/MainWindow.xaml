            <TextBox Name="xres" HorizontalAlignment="Left" Height="27" Margin="174,118,0,0" TextWrapping="Wrap" Text="900" VerticalAlignment="Top" Width="66" Background="#FF2E2E2E" BorderThickness="2" BorderBrush="#FFD7D8D8" Foreground="#FFEAEAEA" FontSize="18" TextAlignment="Center"/>
            <TextBox Name="yres" HorizontalAlignment="Left" Height="27" Margin="271,118,0,0" TextWrapping="Wrap" Text="600" VerticalAlignment="Top" Width="66" Background="#FF2E2E2E" BorderThickness="2" BorderBrush="#FFD7D8D8" Foreground="#FFEAEAEA" FontSize="18" TextAlignment="Center"/>
