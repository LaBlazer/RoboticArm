            <Label Content="Uptime:" HorizontalAlignment="Left" Margin="247,81,0,0" VerticalAlignment="Top" Foreground="#FFECECEC" FontSize="16" FontWeight="Bold"/>
            <Label Content="Power consumption:" HorizontalAlignment="Left" Margin="450,45,0,0" VerticalAlignment="Top" Foreground="#FFECECEC" FontSize="16" FontWeight="Bold" RenderTransformOrigin="0.268,1.032"/>
