            <Label Content="Fan speed:" HorizontalAlignment="Left" Margin="450,153,0,0" VerticalAlignment="Top" Foreground="#FFECECEC" FontSize="16" FontWeight="Bold"/>
            <Label Name="status" Content="err" HorizontalAlignment="Left" Margin="357,45,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#FF609DD1" FontWeight="Bold"/>
            <Label  x:Name="latency" Content="err" HorizontalAlignment="Left" Margin="357,118,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#FF609DD1" FontWeight="Bold"/>
            <Label Name="cpuusage" Content="err" HorizontalAlignment="Left" Margin="667,81,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#FF609DD1" FontWeight="Bold" RenderTransformOrigin="0.467,0.467"/>
            <Label Name="uptime" Content="err" HorizontalAlignment="Left" Margin="357,81,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#FF609DD1" FontWeight="Bold"/>
            <Label Name="temperature" Content="err" HorizontalAlignment="Left" Margin="667,118,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#FF609DD1" FontWeight="Bold"/>
            <Label Content="err" HorizontalAlignment="Left" Margin="667,46,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#FF609DD1" FontWeight="Bold"/>
