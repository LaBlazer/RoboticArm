            <Label x:Name="baseRv" Content="0" HorizontalAlignment="Left" Margin="259,20,0,0" VerticalAlignment="Top" FontSize="18" Foreground="#FF609DD1" FontWeight="Bold" />
            <Label x:Name="elb1v" Content="0" HorizontalAlignment="Left" Margin="259,98,0,0" VerticalAlignment="Top" FontSize="18" Foreground="#FF609DD1" FontWeight="Bold"/>
